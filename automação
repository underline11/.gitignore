import pyautogui
import time

pyautogui.PAUSE = 1  # Pausa de 1 segundo entre cada comando

# Abrir o Google
pyautogui.press('win')
pyautogui.write("google")
pyautogui.press('enter')
time.sleep(2)

pyautogui.hotkey("ctrl", "shift", "N")  # Corrigido: 'pyautogui' em vez de 'payautogui'
time.sleep(2)
pyautogui.write('https://mail.google.com/mail/u/0/#inbox')
pyautogui.press('enter')
time.sleep(10)  # Espera para o Gmail carregar
pyautogui.write("coloque seu email")
pyautogui.press('enter')
time.sleep(5)
pyautogui.write("senha")
pyautogui.press('enter')
time.sleep(13)  
pyautogui.click(35, 178)
time.sleep(20)

pyautogui.write("email")
time.sleep(10)
pyautogui.click(1248, 349)
pyautogui.write("saudação")
time.sleep(4)
pyautogui.press('tab')
pyautogui.write("oi professor")
time.sleep(5)
pyautogui.click(870, 684)
